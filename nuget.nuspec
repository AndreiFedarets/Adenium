<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>Layex</id>
    <version>$(majorVersion).$(minorVersion).$(buildVersion).$(revisionVersion)$(suffixVersion)</version>
    <title>$(productName)</title>
    <authors>$(companyName)</authors>
    <owners>$(companyName)</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>$(productUrl)</projectUrl>
    <description>$(productDescription)</description>
    <releaseNotes></releaseNotes>
    <copyright>$(productCopyright)</copyright>
    <tags>Layex WPF Composite UI</tags>
	<dependencies>
		<group targetFramework="net40">
			<dependency id="Caliburn.Micro" version="3.0.0" />
			<dependency id="Unity" version="5.0.0" />
		</group>
	</dependencies>
  </metadata>
  <files>
	<file src="build\release\net40\*.dll" target="lib\net40" />
  </files>
</package>
